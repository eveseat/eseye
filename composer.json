{
    "name": "eveseat/eseye",
    "description": "A Standalone PHP ESI (EVE Swagger Interface) Client Library",
    "type": "library",
    "require": {
        "php": ">= 7.2|^8.0",
        "ext-gmp": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "guzzlehttp/guzzle": "^6.2|^7.0",
        "nesbot/carbon": "^2.0",
        "monolog/monolog": "^2.0",
        "predis/predis": "^1.1",
        "web-token/jwt-easy": "^2.1",
        "web-token/jwt-signature-algorithm-hmac": "^2.1",
        "web-token/jwt-signature-algorithm-rsa": "^2.1",
        "web-token/jwt-signature-algorithm-ecdsa": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "mikey179/vfsstream": "~1",
        "m6web/redis-mock": "^5.0",
        "codeclimate/php-test-reporter": "dev-master"
    },
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Leon Jacobs",
            "email": "leonja511@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Seat\\Eseye\\": "src/"
        },
        "files": [
            "src/Helpers/helpers.php"
        ]
    },
    "bin": [
        "bin/tokengenerator"
   ]
}
